<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite Selenium" parallel="tests">

	<test name="Test  on Chrome">
		<parameter name="BrowserName" value="chrome"></parameter>
		<classes>
			<class name="pe.edu.galaxy.MultiBrowser" />
		</classes>
	</test>

	<test name="Test  on Firefox">
		<parameter name="BrowserName" value="firefox"></parameter>
		<classes>
			<class name="pe.edu.galaxy.MultiBrowser" />
		</classes>
	</test>

	<test name="Test  on Opera">
		<parameter name="BrowserName" value="opera"></parameter>
		<classes>
			<class name="pe.edu.galaxy.MultiBrowser" />
		</classes>
	</test>


	<test name="Test  on Iexplorer">
		<parameter name="BrowserName" value="ie"></parameter>
		<classes>
			<class name="pe.edu.galaxy.MultiBrowser" />
		</classes>
	</test>

	<test name="Test  on Edge">
		<parameter name="BrowserName" value="edge"></parameter>
		<classes>
			<class name="pe.edu.galaxy.MultiBrowser" />
		</classes>
	</test>
</suite>
